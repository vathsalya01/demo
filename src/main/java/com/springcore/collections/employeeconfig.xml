<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">
		
		
	<bean name="emp" class="com.springcore.collections.Employee">
		<property name="ename" value="Dinesh"/>
		<property name="ephones">
			<list>
				<value>4857834</value>
				<value>38759930</value>
				<value>48574869</value>
				<null/>
			</list>
		</property>
		
		<property name="eaddress">
			<set>
				<value>Delhi</value>
				<value>Lucknow</value>
			</set>
		</property>
		
		<property name="ecourse">
			<map>
				<entry key="Java" value="2"/>
				<entry key="Python" value="3"/>
				<entry key="C" value="2"/>
				
			</map>		
		</property>
	
	</bean>

</beans>