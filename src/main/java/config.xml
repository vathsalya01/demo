<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">
		
	<bean id="student1" class="com.springmaven.Student">
		
		<property name="sid" value="201"/>
		<property name="sname" value="Daniel"/>
		<property name="saddr" value= "Hyderabad"/>
		
		
	</bean>
	
	<bean class="com.springmaven.Student" name="student2" p:sid="202" p:sname="Emma" p:saddr="Banglore"></bean>
	
	
